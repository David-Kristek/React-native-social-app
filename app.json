{
  "expo": {
    "name": "simple social",
    "slug": "simple-social",
    "version": "2.0",
    "orientation": "portrait",
    "icon": "./assets/icona.png",
    "splash": {
      "image": "./assets/icona.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icona.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "david.kristek.app",
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/icona.png"
    }
  }
}
