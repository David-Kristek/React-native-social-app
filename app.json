{
  "expo": {
    "name": "social site",
    "slug": "social-site",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icona.png",
    "splash": {
      "image": "./assets/icona.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icona.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "david.kristek.app"
    },
    "web": {
      "favicon": "./assets/icona.png"
    }
  }
}
